<!> checkpoint
phasej 51 0 #{
	saction 0
        $bookctrl()
        $pipes(3)
        ~ async _ <> gcr2 lerpt(0, 6, 720p, 360p) 9 <> {
            delay(300)
            for(36s)
            root zero
        } $book(.y Lplayer, nrvelocity(pxy(-2.3, sine(4, 0.5, t))))
    saction 0
        sync _ <> $obj1(2)
        wait 4
        sync _ <> $obj1(-2)
        wait 2
        sync _ <> $obj1(0)
        wait 1
        sync _ <> $obj1(3)
    saction 2
