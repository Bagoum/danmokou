        wait 1
        sync _ <> $obj1(-0.5)
        wait 2
        sync _ <> $obj5(-3)
    saction 3
        sync _ <> $obj2(3)
        wait 1.4
        sync _ <> $obj2(-2.5)
    saction 3 ##4
        ~ _ 0.6 sync _ <> $obj3(-2)
        async _ <;3:> gcr2 120 3 <;-1.4:> {
        } $obj1(y)
    saction 4
