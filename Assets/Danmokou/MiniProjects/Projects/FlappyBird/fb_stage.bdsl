        wait 1.3
        sync _ <> $obj3(0)
        wait 1.4
        sync _ <> $obj5(-2)
    saction 2
        sync _ <> $obj2(1)
        wait 2
        async _ <> gcr2 80 3 <> {
            p this
        } gsr2 2 <> {
            bindLR
        } $obj5((&lr * (3 - p)))
        wait 1
        async _ <> gcr2 240 3 <> {
        } $obj3(.x Lplayer)
        
